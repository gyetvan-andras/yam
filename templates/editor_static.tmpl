			<input type='text' class="yam-color yam-30" v-model="pm.tintColor" id="img_color_tint"><div id="cp_img_color_tint"></div>
		</div>
		<div class="yam-f-row yam-f-header">
			<div class="yam-15">&nbsp;</div>
			<div class="yam-title yam-60">Filters</div>
		</div>
		<div class="yam-f-scroll">
			<div class="yam-f-row">
				<div class="yam-5">&nbsp;</div>
				<div class="yam-10">
					<input type="radio" value="original" v-model="pm.presetMode" class="yam-radio">
				</div>
				<a class='preset-col90 text-center' v-on:click="applyPreset('original')" href="javascript:void(0);">
					<img class="yam-preview-img" src="images/sample_small.jpg">
					<p class="yam-title yam-90">Original</p>
				</a>

			</div>

			<div v-for='preset in presets' class="yam-f-row">
