			<a v-on:click="$root.sendBack()" href="javascript:void(0)">&nbsp;&nbsp;<i class="icon-send-back yam-label"> </i></a>
			<a v-on:click="$root.bringToFront()" href="javascript:void(0)">&nbsp;<i class="icon-bring-front yam-label"> </i></a>
			<a v-on:click="$root.deleteObject()" href="javascript:void(0)">&nbsp;<i class="fa fa-trash-o yam-label"> </i></a>
		</div>
		<!-- <div class="yam-separator"></div> -->

		<div class="yam-f-row yam-f-header">
			<div class="yam-15">&nbsp;</div>
			<div class="yam-title yam-60">Tint</div>
		</div>
