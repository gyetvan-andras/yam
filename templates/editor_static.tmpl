			<div class="yam-15">&nbsp;</div>
			<div class="yam-10">
				<!-- <input type="radio" value="3" v-model="pm.fillMode"  class="yam-radio"> -->
				<input type="checkbox" v-model="pm.isCutOut" class="yam-checkbox">
			</div>
			<div class="yam-label yam-30">Cut-out</div>
			<!-- <input type='text' class="yam-color yam-30" value="#00ff00" id="txt_color_cut_out"><div id="cp_txt_color_cut_out"></div> -->
		</div>
		<div class="yam-row">
			<div class="yam-15">&nbsp;</div>
			<div class="yam-10">
				<input type="radio" value="0" v-model="pm.fillMode"  class="yam-radio">
			</div>
			<div class="yam-label yam-30">None</div>
		</div>

		<!-- <div class="yam-separator"></div> -->

		<div class="yam-row yam-header">
			<div class="yam-15">
				<img class="yam-header-img" src="assets/txt_stroke.png">
			</div>
			<div class="yam-title yam-60">Outline</div>
		</div>
		<div class="yam-row">
			<div class="yam-15">&nbsp;</div>
			<div class="yam-10">
				<input type="checkbox" v-model="pm.drawOutline" class="yam-checkbox">
			</div>
			<div class="yam-label yam-30">Color</div>
			<input type='text' class="yam-color yam-30" v-model="pm.outlineColor" id="txt_color_outline"><div id="cp_txt_color_outline"></div>
		</div>
		<div class="yam-row">
			<div class="yam-25">&nbsp;</div>
			<div class="yam-label yam-30">Thickness</div>
			<input type='range' class="yam-range yam-30" v-model="pm.outlineThickness" min="1" max="128">
		</div>

		<!-- <div class="yam-separator"></div> -->

		<div class="yam-row yam-header">
			<div class="yam-15">
				<img class="yam-header-img" src="assets/txt_shadow.png">
			</div>
			<div class="yam-title yam-60">Shadow</div>
		</div>
		<div class="yam-row">
			<div class="yam-15">&nbsp;</div>
			<div class="yam-10">
				<input type="checkbox" v-model="pm.dropShadow" class="yam-checkbox">
			</div>
			<div class="yam-label yam-30">Drop</div>
			<input type='text' class="yam-color yam-30" v-model="pm.dropShadowColor" id="txt_color_shadow"><div id="cp_txt_color_shadow"></div>
		</div>
		<div class="yam-row">
			<div class="yam-25">&nbsp;</div>
			<div class="yam-label yam-30">Blurr</div>
			<input type='range' class="yam-range yam-30" v-model="pm.dropShadowBlur" min="1" max="256">
		</div>
		<div class="yam-row">
			<div class="yam-25">&nbsp;</div>
			<div class="yam-label yam-30">Distance</div>
			<input type='range' class="yam-range yam-30" v-model="pm.dropShadowDistance" min="0" max="512">
		</div>
		<div class="yam-row">
			<div class="yam-25">&nbsp;</div>
			<div class="yam-label yam-30">Angle</div>
			<input type='range' class="yam-range yam-30" v-model="pm.dropShadowAngle" min="0" max="359">
		</div>
	</div>
</template>

